{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "webpack-dev-server",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "wdio": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-lcov wdio"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "sourceMap": true,
    "instrument": false,
    "cache": true,
    "all": true,
    "extension": [
      ".jsx",
      ".js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "geckodriver": "^1.12.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "selenium-standalone": "^6.15.3",
    "source-map-support": "^0.5.9",
    "wdio-mocha-framework": "^0.6.4",
    "webdriverio": "^4.14.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
